{
  "name": "fordypningsprosjekt-1-liena-grytsyna",
  "version": "1.0.0",
  "description": "Frontend portfolio project by Liena Grytsyna",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass:watch": "sass --watch src/styles/scss:src/styles/css",
    "sass:build": "sass src/styles/scss/main.scss:src/styles/css/main.css --no-source-map --style=compressed",
    "start": "npm run sass:build && parcel src/index.html --port 3000",
    "dev": "npm-run-all --parallel sass:watch start",
    "build": "npm run sass:build && parcel build src/index.html",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,scss,html}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VaagenIM/fordypningsprosjekt-1-liena-grytsyna.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/VaagenIM/fordypningsprosjekt-1-liena-grytsyna/issues"
  },
  "homepage": "https://github.com/VaagenIM/fordypningsprosjekt-1-liena-grytsyna#readme",
  "dependencies": {
    "sass": "^1.93.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.16.0",
    "prettier": "^3.1.1"
  }
}
